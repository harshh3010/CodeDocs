        <HBox alignment="CENTER" prefHeight="668.0" prefWidth="941.0" VBox.vgrow="ALWAYS">
            <children>
                <ScrollPane fx:id="scroll" prefHeight="570.0" prefWidth="900.0" styleClass="transparent"
